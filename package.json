{
	"name": "dub",
	"description": "Package manager for D packages",
	"license": "MIT",
	"copyright": "Copyright 2012 rejectedsoftware e.K.",
	"authors": [
		"Matthias Dondorff",
		"Sönke Ludwig"
	],
	"targetPath": "bin",
	"configurations": [
		{
			"name": "application",
			"targetType": "executable",
			"mainSourceFile": "source/app.d",
			"libs": ["curl"],
			"libs-posix-dmd": ["phobos2"],
			"copyFiles-windows": ["curllib.dll", "libeay32.dll", "openldap.dll", "ssleay32.dll"],
			"versions": ["DubUseCurl"]
		},
		{
			"name": "library",
			"targetType": "library",
			"excludedSourceFiles": ["source/app.d"],
			"libs": ["curl"],
			"libs-posix-dmd": ["phobos2"],
			"copyFiles-windows": ["curllib.dll", "libeay32.dll", "openldap.dll", "ssleay32.dll"],
			"versions": ["DubUseCurl"]
		},
		{
			"name": "library-nonet",
			"targetType": "library",
			"dependencies": {
				"vibe-d": {"version": "~master", "optional": true}
			},
			"excludedSourceFiles": ["source/app.d"]
		}
	]
}